<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:prism="http://prismlibrary.com"
             prism:ViewModelLocator.AutowireViewModel="True"
             x:Class="PrismApp.Views.AnimationPage">
    <StackLayout Orientation="Vertical" x:Name="LayoutRoot">
        <Button x:Name="SpinButton" Text="Spin" Clicked="SpinButton_Clicked" Margin="10"/>
        <Button x:Name="CompoundButton" Text="Compound" Clicked="CompoundButton_Clicked" Margin="10"/>
        <Button x:Name="ZoomyFadeButton" Text="Zoomy Fade" Clicked="ZoomyFadeButton_Clicked" Margin="10"/>
        <Button x:Name="BooshButton" Text="Boosh" Clicked="BooshButton_Clicked" Margin="10"/>
    </StackLayout>
</ContentPage>